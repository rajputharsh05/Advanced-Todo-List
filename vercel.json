{
    "version": 2,
    "builds": [
      {
        "src": "to-do-list/src/api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "to-do-list/build",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "to-do-list/src/api/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "to-do-list/build/$1"
      }
    ]
  }
  